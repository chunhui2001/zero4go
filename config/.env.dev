### [app settings]
GIN_ENV=local
APP_NAME=zero4go
APP_PORT=0.0.0.0:8080
TZ=Asia/Shanghai
RPC_PORT=0.0.0.0:50051

### [logger settings]
LOG_LEVEL=debug
LOG_COLOR=false

### [graph server]
GRAPHQL_ENABLE=false
GRAPHQL_SERVER_URI=/graphql
GRAPHQL_PLAYGROUND_URI=/graphiql

### [redis]
#REDIS_MODE="disable"
REDIS_MODE="standalone"
REDIS_HOST=redis7.2.3:6379
# REDIS_URL=redis://<user>:<pass>@localhost:6379/<db>
#REDIS_PASSWORD=Cc
REDIS_PASSWORD=
REDIS_DATABASE=0
REDIS_MAX_IDLE=30
REDIS_MAX_ACTIVE=30
REDIS_IDLE_TIMEOUT=200s
### just redis sentinel mode have master name
REDIS_MASTER_NAME=__my_redis_master_name_here__
REDIS_ADDRS=127.0.0.1:6381,127.0.0.1:6382,127.0.0.1:6383,127.0.0.4:6384,127.0.0.1:6385
### To route commands by latency or randomly, enable one of the following.
REDIS_ROUTE_BY_LATENCY=false
REDIS_ROUTE_RANDOMLY=true

### [mysql settings]
MYSQL_ENABLE=true
#MYSQL_CONN_OPTS=timeout=90s&interpolateParams=true&multiStatements=true&charset=utf8&autocommit=false&parseTime=True&loc=UTC
MYSQL_CONN_OPTS=timeout=90s&interpolateParams=true&multiStatements=true&charset=utf8&autocommit=true&parseTime=True&loc=Asia%2FShanghai
MYSQL_SERVER=mysql_8.0:3306
MYSQL_DATABASE=zero4rs_db
MYSQL_USER_NAME=keesh
MYSQL_PASSWD=Cc
MYSQL_INIT_SCRIPT=./resources/msyql_script_init
MYSQL_UPDATE_SCRIPT=./resources/msyql_script_update
MYSQL_MAPPER_LOCATION="./META-INF/mappers/*.txt"

### [Kafka setting]
KAFKA_ENABLE=true
BOOTSTRAP_SERVERS=kafka_2.6.0:9093
MESSAGE_TIMEOUT_MS=5000

### [ElasticSearch setting]
ES_ICCO_ENABLE=false
ES_ICCO_SERVERS=http://localhost:9211
ES_ICCO_DSL_LOCATION=./META-INF/es_dsl

### [OpenEs setting]
ES_OPEN_ENABLE=false
ES_OPEN_SERVERS=http://localhost:9411
ES_OPEN_DSL_LOCATION=./META-INF/es_dsl

### [zookeeper settings]
ZOOKEEPER_ENABLED=false
ZOOKEEPER_SERVERS=127.0.0.1:2181,127.0.0.1:2182,127.0.0.1:2183

